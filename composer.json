{
    "name": "giannisftaras/smartthings",
    "description": "A PHP wrapper for the Samsung Smart Things API",
    "version": "1.0.0",
    "type": "library",
    "homepage": "https://github.com/giannisftaras/smartthings",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Giannis Ftaras",
            "email": "me@giannisftaras.dev",
            "homepage": "https://giannisftaras.dev",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "7.0"
    },
    "autoload": {
        "psr-4": {
            "smartThings\\": "src/"
        },
        "files": [
            "src/smartThings/smartThingsAPI.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "smartThings\\smartThingsServiceProvider"
            ]
        }
    }
}
